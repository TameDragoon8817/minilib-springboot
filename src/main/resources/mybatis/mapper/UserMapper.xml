<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mavenbase.minilibspringboot.dao.UserMapper">
    <select id="queryUserByName" parameterType="string" resultType="com.mavenbase.minilibspringboot.pojo.User">
        select * from book.securityusers where name = #{name}
    </select>
</mapper>